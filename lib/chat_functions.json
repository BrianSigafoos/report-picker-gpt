{
  "name": "get_report_links",
  "description": "Get report links",
  "parameters": {
    "type": "object",
    "properties": {
      "reports": {
        "type": "array",
        "maxItems": 3,
        "items": {
          "type": "object",
          "properties": {
            "key": {
              "type": "string",
              "description": "The key of the report"
            },
            "filters": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "key": {
                    "type": "string",
                    "enum": [
                      "startDate",
                      "endDate",
                      "amount",
                      "location",
                      "device",
                      "employee",
                      "paymentMethod"
                    ],
                    "description": "The key of the filter"
                  },
                  "value": {
                    "type": "string",
                    "description": "The value of the filter. If the filter is a date, it must be in the format YYYY-MM-DD"
                  }
                },
                "required": [
                  "key",
                  "value"
                ]
              },
              "description": "The filters of this report to apply"
            },
            "confidence": {
              "type": "number",
              "description": "Confidence level that this is the best report to answer the question."
            }
          },
          "required": [
            "key",
            "filters"
          ]
        }
      },
      "error": {
        "type": "string",
        "description": "An optional error message"
      }
    }
  }
}
